{
  "name": "backfeed",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others --names \"GRAPHQL-CODEGEN,CLIENT\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn graphql:generate:watch\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "graphql:generate": "NODE_PATH=src/ dotenv -e .env.development -- graphql-codegen --config src/lib/graphql/codegen.config.ts",
    "graphql:generate:watch": "yarn graphql:generate -w",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@walletconnect/jsonrpc-types": "^1.0.3",
    "@walletconnect/jsonrpc-utils": "^1.0.8",
    "concurrently": "^8.0.1",
    "dotenv": "^16.1.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.1.4",
    "@chakra-ui/react": "^2.7.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.0",
    "@rainbow-me/rainbowkit": "^1.0.1",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "dayjs": "^1.11.7",
    "framer-motion": "^10.12.16",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "lodash.merge": "^4.6.2",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.2",
    "react-icons": "^4.9.0",
    "viem": "^0.3.41",
    "wagmi": "^1.0.9",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  }
}
