{
  "name": "backfeed",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && concurrently --kill-others --names \"GRAPHQL-CODEGEN,PANDA,CLIENT\" -c \"bgBlue.bold,bgYellow.bold,bgMagenta.bold\" \"bun graphql:generate:watch\" \"bun panda --watch\" \"bun dev:next\"",
    "dev:next": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "check": "biome check src",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "test": "bun test --env-file .env.local --env-file .env.test .test",
    "test:e2e": "NODE_ENV=test bun run --env-file .env.local --env-file .env.test playwright test",
    "graphql:generate": "NODE_PATH=src/ dotenv -e .env.development -- graphql-codegen --config src/lib/graphql/codegen.config.ts",
    "graphql:generate:watch": "bun graphql:generate -w",
    "knip": "knip-bun --cache --tags=-knipignore",
    "prepare": "panda codegen && husky"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/import-types-preset": "^3.0.1",
    "@graphql-codegen/plugin-helpers": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-msw": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@graphql-codegen/typescript-react-query": "^6.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@happy-dom/global-registrator": "^20.0.10",
    "@pandacss/dev": "^1.5.0",
    "@parcel/watcher": "^2.5.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.2",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.2",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "knip": "^5.69.0",
    "msw": "^2.12.1",
    "next-router-mock": "^1.0.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ark-ui/react": "^5.27.1",
    "@auth/core": "^0.34.3",
    "@omnidev/sigil": "https://pkg.pr.new/omnidotdev/sigil/@omnidev/sigil@178@dd587c7",
    "@polar-sh/sdk": "^0.41.2",
    "@react-email/components": "^1.0.0",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-pacer": "^0.17.0",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "dayjs": "^1.11.19",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.3",
    "motion": "^12.23.24",
    "ms": "^2.1.3",
    "next": "^16.0.1",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-hook": "^6.0.1",
    "recharts": "^3.4.1",
    "resend": "^6.4.2",
    "ts-pattern": "^5.9.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "bufferutil",
    "esbuild",
    "keccak",
    "msw",
    "sharp",
    "utf-8-validate"
  ]
}
