{
  "name": "backfeed",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && concurrently --kill-others --names \"GRAPHQL-CODEGEN,PANDA,CLIENT\" -c \"bgBlue.bold,bgYellow.bold,bgMagenta.bold\" \"bun graphql:generate:watch\" \"bun panda --watch\" \"bun dev:next\"",
    "dev:next": "next dev --turbo --experimental-https",
    "build": "next build",
    "start": "next start",
    "check": "biome check src",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "test": "bun test --env-file .env.local --env-file .env.test .test",
    "test:e2e": "NODE_ENV=test bun run --env-file .env.local --env-file .env.test playwright test",
    "graphql:generate": "NODE_PATH=src/ dotenv -e .env.development -- graphql-codegen --config src/lib/graphql/codegen.config.ts",
    "graphql:generate:watch": "bun graphql:generate -w",
    "knip": "knip-bun --cache --tags=-knipignore",
    "prepare": "panda codegen && husky"
  },
  "msw": {
    "workerDirectory": ["public"]
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@graphql-codegen/plugin-helpers": "^5.1.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-msw": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@graphql-codegen/typescript-react-query": "^6.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@happy-dom/global-registrator": "^17.4.4",
    "@pandacss/dev": "^0.53.3",
    "@parcel/watcher": "^2.5.1",
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.8",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "husky": "^9.1.7",
    "knip": "^5.46.3",
    "msw": "^2.7.3",
    "next-router-mock": "^0.9.13",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@ark-ui/react": "^5.5.0",
    "@auth/core": "^0.39.0",
    "@omnidev/sigil": "^0.15.2",
    "@polar-sh/sdk": "^0.32.3",
    "@react-email/components": "^0.0.35",
    "@tanstack/react-form": "^1.2.1",
    "@tanstack/react-pacer": "^0.3.0",
    "@tanstack/react-query": "^5.70.0",
    "@tanstack/react-query-devtools": "^5.70.0",
    "@tanstack/react-table": "^8.21.2",
    "@theme-toggles/react": "^4.1.0",
    "dayjs": "^1.11.13",
    "flags": "^3.2.0",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "ms": "^2.1.3",
    "next": "^15.3.0",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-hook": "^6.0.0",
    "recharts": "^2.15.1",
    "resend": "^4.2.0",
    "ts-pattern": "^5.7.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "bufferutil",
    "esbuild",
    "keccak",
    "msw",
    "sharp",
    "utf-8-validate"
  ]
}
