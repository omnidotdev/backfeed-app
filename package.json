{
  "name": "backfeed",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --names \"GRAPHQL-CODEGEN,PANDA,CLIENT\" -c \"bgBlue.bold,bgYellow.bold,bgMagenta.bold\" \"bun graphql:generate:watch\" \"bun panda --watch\" \"bun dev:vite\"",
    "dev:vite": "vite dev",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "check": "biome check",
    "format": "biome format --write",
    "lint": "biome lint",
    "test": "bun test --env-file .env.local --env-file .env.test .test",
    "test:e2e": "NODE_ENV=test bun run --env-file .env.local --env-file .env.test playwright test",
    "graphql:generate": "NODE_PATH=src/ dotenv -e .env.development -- graphql-codegen --config src/lib/graphql/codegen.config.ts",
    "graphql:generate:watch": "bun graphql:generate -w",
    "knip": "knip-bun --cache --tags=-knipignore",
    "prepare": "panda codegen && husky"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "dependencies": {
    "@ark-ui/react": "^5.30.0",
    "@auth/core": "^0.40.0",
    "@omnidev/sigil": "^0.16.0",
    "@react-email/components": "^1.0.2",
    "@tanstack/react-form": "^1.27.5",
    "@tanstack/react-pacer": "^0.18.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.141.8",
    "@tanstack/react-router-ssr-query": "^1.141.8",
    "@tanstack/react-start": "^1.142.5",
    "@tanstack/react-table": "^8.21.3",
    "dayjs": "^1.11.19",
    "graphql": "^16.12.0",
    "graphql-request": "^7.4.0",
    "motion": "^12.23.26",
    "ms": "^2.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hotkeys-hook": "^5.2.1",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-hook": "^6.0.1",
    "recharts": "^3.6.0",
    "resend": "^6.6.0",
    "stripe": "^20.1.0",
    "ts-pattern": "^5.9.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/import-types-preset": "^3.0.1",
    "@graphql-codegen/plugin-helpers": "^6.1.0",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-msw": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@graphql-codegen/typescript-react-query": "^6.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@happy-dom/global-registrator": "^20.0.11",
    "@pandacss/dev": "^1.7.1",
    "@parcel/watcher": "^2.5.1",
    "@playwright/test": "^1.57.0",
    "@tanstack/devtools-vite": "^0.3.12",
    "@tanstack/nitro-v2-vite-plugin": "^1.141.0",
    "@tanstack/react-devtools": "^0.8.4",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router-devtools": "^1.141.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.5",
    "@types/ms": "^2.1.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "husky": "^9.1.7",
    "knip": "^5.76.1",
    "msw": "^2.12.4",
    "typescript": "^5.9.3",
    "vite": "8.0.0-beta.0",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@parcel/watcher",
    "esbuild",
    "msw"
  ]
}
